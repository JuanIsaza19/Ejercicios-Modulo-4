clase pública Principal {
    public static void main(String[] args) {
        int numeroMientras = 0;

        while (numeroMientras < 3) {
            numeroMientras++;
            System.out.println("numeroMientras ahora es:"+ numeroMientras);
        }
    }
}

-------------------------------------------------- -------------------------------------------------

clase pública Principal {
    public static void main(String[] args) {
        int numeroDoMientras = 10;

        hacer {
            numeroDoMientras++;
            System.out.println("numeroDoWhile ahora es:" + numeroDoWhile);
        }mientras (numeroDoWhile < 3);

    }
}
    
