 EJERCICIOS MODULO 4
   
   
clase pública Principal {
    public static void main(String[] args) {
        int numerolf = 10;

        si (numerolf >= 1) {
            System.out.println("es positivo");
        }
        si (numerolf == 0) {
            Sistema.salida.println("Es 0");
        }
        si (numerolf <= -1){
            System.out.println("es negativo");
        }
    }
}    
  
